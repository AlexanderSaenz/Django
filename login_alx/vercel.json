{
  "version": 2,
  "builds": [
    {
      "src": "login_alx/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "login_alx/wsgi.py" }
  ]
}
